                <li><symbol>cowar</symbol></li>
                <li><symbol>cry</symbol></li>
            </prefixSymbols>
            <suffixSymbols>
                <li><symbol>whiner</symbol></li>
                <li><symbol>wimp</symbol></li>
            </suffixSymbols>
        </symbolPack>
    </GeneDef>
</Defs>
